#include<iostream>
 
using namespace std;
 
int main()
{
    long long N;
    long long M;
    
    cin >> N;
    cin >> M;
    
    cout << N << " + " << M << " = " << N + M << endl << endl;
    cout << N << " - " << M << " = " << N - M << endl << endl;
    cout << N << " * " << M << " = " << N * M << endl << endl;
    cout << N << " / " << M << " = " << N / M << endl << endl;
    cout << N << " % " << M << " = " << N % M << endl << endl;
}